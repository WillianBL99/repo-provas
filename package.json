{
  "name": "typescript-api-tamplate",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/server.js",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production NODE_TLS_REJECT_UNAUTHORIZED=0 node dist/src/server.js",
    "build": "NODE_ENV=production npx tsc --outDir dist && prisma migrate deploy",
    "dev": "NODE_ENV=development npx nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"ts-node src/server.ts\"",
    "test": "NODE_ENV=test && dotenv -e .env.test prisma migrate dev && NODE_OPTIONS=--experimental-vm-modules dotenv -e .env.test jest -i"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WillianBL99/typescript-api-tamplate.git"
  },
